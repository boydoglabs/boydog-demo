{
  "name": "boydog-demo",
  "version": "0.0.2",
  "description": "A boilerplate for BoyDog projects",
  "main": "server.js",
  "scripts": {
    "start": "node ./server.js",
    "forever": "forever --uid \"boydog-demo\" ./server.js",
    "dev": "npm install --no-save ./git_modules/boydog/ && npm run -s _dev:boydog && npm run -s _dev:boydog-client && nodemon ./server.js",
    "format": "prettier --write \"**/*.js\"",
    "test": "mocha --timeout 30000",
    "_dev:boydog-client": "cd ./git_modules/boydog-client && npm install --no-save ../sharedb-attribute-binding/ && cd ../../",
    "_dev:boydog": "cd ./git_modules/boydog && npm install --no-save ../boydog-client/ && cd ../../"
  },
  "author": "BoyDog (boy.dog)",
  "contributors": [
    "BoyDog"
  ],
  "license": "MIT",
  "dependencies": {
    "boydog": "^1.1.29",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "mocha": "^6.1.4",
    "nightmare": "^3.0.2",
    "prettier": "^1.18.2"
  }
}
